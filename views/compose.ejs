
<%- include('partials/header'); -%>
<h1>Compose</h1>

<form  action="/compose" method="post">
  <div class="mb-3">
    <label for="newpost" class="form-label">Title</label>
    <input type="text" class="form-control" id="newpost"  name="newblog">
  </div>
  <div class="mb-3">
    <label for="newcontent" class="form-label">Post</label>
    <textarea class="form-control" id="newcontent" rows="3" name="postbody"></textarea>
    <br>
  </div>
<button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include('partials/footer'); -%>
